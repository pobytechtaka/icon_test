{
  "name": "argo-icon",
  "version": "1.0.0",
  "description": "argo icon 라이브러리",
  "scripts": {
    "transform": "node script/svgToComponent.cjs && node script/componentExport.cjs",
    "build": "yarn transform && yarn vite build"
  },
  "author": "",
  "type": "module",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./*": {
      "types": "./build/*.d.ts",
      "import": "./build/*.js",
      "require": "./build/index.cjs"
    },
    "./build/*": {
      "types": "./build/*.d.ts",
      "import": "./build/*.js",
      "require": "./build/index.cjs"
    }
  },
  "peerDependencies": {
    "react": "^17"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@vitejs/plugin-react": "^2.2.0",
    "glob": "^11.0.0",
    "typescript": "^5.4.3",
    "vite": "^3.2.5",
    "vite-plugin-dts": "^3.9.1",
    "@svgr/plugin-svgo": "^8.1.0"
  }
}
